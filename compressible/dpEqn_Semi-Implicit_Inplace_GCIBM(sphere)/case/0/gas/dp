FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      dp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    TOP
    {
        type            fixedValue;
        value           uniform 0.0;
    }

    BOTTOM
    {
        type            zeroGradient;
    }

    FRONT
    {
        type            empty;
    }

    BACK
    {
        type            empty;
    }

    LEFT
    {
        type            zeroGradient;
    }

    RIGHT
    {
        type            zeroGradient;
    }
}

