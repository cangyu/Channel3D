FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
    TOP
    {
        type            zeroGradient;
    }

    BOTTOM
    {
        type            zeroGradient;
    }

    AROUND
    {
        type            zeroGradient;
    }
}

